# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# Azure AI Search Configuration (Required)
AZURE_AI_SEARCH_ENDPOINT=https://your-search-service.search.windows.net
AZURE_AI_SEARCH_KEY=your-search-admin-key

# Azure OpenAI Configuration (Required for GPT-enhanced answers and hybrid search)
AZURE_OPENAI_ENDPOINT=https://your-openai-service.openai.azure.com
AZURE_OPENAI_KEY=your-openai-key

# OpenAI Model Deployments (Optional - defaults shown)
AZURE_OPENAI_COMPLETION_DEPLOYMENT=gpt-4.1
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding-ada-002

# Test Query Configuration (Required)
TEST_QUERY=How can I create a SharePoint Agent? What is the best way?

# Logging Configuration (Optional)
LOG_LEVEL=INFO

# Example values:
# AZURE_AI_SEARCH_ENDPOINT=https://mycompany-search.search.windows.net
# AZURE_AI_SEARCH_KEY=ABC123DEF456GHI789JKL012MNO345PQR678STU901VWX234YZ
# AZURE_OPENAI_ENDPOINT=https://mycompany-openai.openai.azure.com
# AZURE_OPENAI_KEY=def456ghi789jkl012mno345pqr678stu901vwx234yz567abc890
